---
layout: home
---

#articles_list
  %ul
    {% for category in site.categories %}
    %li.category_title
      %h3
        {{ category[0] }}
      %ul
        {% for post in category[1] %}
        %li.post_list
          %a{:href => "{{ post.url }}"} 
            {{ post.title }}
        {% endfor %}
    {% endfor %}


